 type=\"text\" v-model=\"description\" name=\"description\" placeholder=\"Product Description\">\n            <input type=\"text\" v-model=\"price\" name=\"price\" placeholder=\"Product Price\">\n            <input type=\"submit\" value=\"Submit\" class=\"btn\">\n        </form>\n\n    </div>\n</template>\n\n<script>\nimport { v4 as uuidv4 } from 'uuid';\n\nexport default {\n    name: \"AddProduct\",\n    data() {\n        return{\n            title: '',\n            price: '',\n            description: ''\n        } \n    }, \n    methods: {\n        addProduct(e) {\n            e.preventDefault();\n            const newProduct = {\n                id: uuidv4(),\n                title: this.title,\n                price: this.price,\n                description: this.description\n            }\n            this.$emit('add-product', newProduct);\n\n            this.title = '';\n            this.price = '';\n            this.description = '';\n        }\n    }\n}\n</script>\n\n<style scoped>\n    form {\n        display: flex;\n    }\n\n    input[type=\"text\"] {\n        flex: 10;\n        padding: 5px;\n    }\n\n    input[type=\"submit\"] {\n        flex: 2;\n    }\n</style>"],"sourceRoot":"src/components"}]}